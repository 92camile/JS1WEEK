var Task = function(name){
	this.name = name;
	this.completed = false;
	
	//this.complted = function() {
		//console.log('completing task: ' + task.name);
		//this.completed = true;
	//}
	//this.save = function(){
		//console.log('saving task: ' + this.name);
	//}
}
//A prototype is a model that displays the appearance and behavior of an application or product early in the development lifecycle.
//name.prototupe.methodname = 
Task.prototype.complete = function () {
	console.log('completing task: ' + this.name);
	this.completed = true;
};

Task.prototype.save = function () {
	console.log('saving Task: ' + this.name);
}

var task1 = new Task('dddd');
var task2 = new Task('dddd');
var task3 = new Task('dddd');
var task4 = new Task('dddd');

task1.complete();
task2.save();
task3.save()
task4.save();
